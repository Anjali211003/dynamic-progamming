class Solution {
public:
    int uniquePaths(int m, int n) {
        if(m==1 || n==1)
        return 1;
        int t[m+1][n+1];
        for(int i=0;i<m;i++){
            for(int j=0;j<n;j++){
                if(!i || !j)
                t[i][j]=1;
            }
        }
        for(int i=1;i<m;i++){
            for(int j=1;j<n;j++){
             t[i][j]=t[i][j-1]+t[i-1][j];
            }
        }
        return t[m-1][n-1];
    }
};
